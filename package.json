{
  "name": "delivery-orm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "module",
    "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/server.ts",
    "test": "vitest",
    "db-create-schema": "npx mikro-orm-esm schema:create --dump > createSchema.sql",
    "db-update-schema": "npx mikro-orm-esm schema:update --dump > updateSchema.sql",
    "db-drop-schema": "npx mikro-orm-esm schema:drop --dump > dropSchema.sql"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mikro-orm/core": "^6.2.9",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/mysql": "^6.2.9",
    "@mikro-orm/reflection": "^6.2.9",
    "@mikro-orm/seeder": "^6.2.9"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.2.9",
    "@types/node": "^20.14.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
